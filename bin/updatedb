#!/bin/bash

rm --force spain-latest.osm.pbf
curl --progress-bar --remote-name https://download.geofabrik.de/europe/spain-latest.osm.pbf
osm2pgsql --create --slim --cache=2048 --multi-geometry --proj=4326 --number-processes=2 --style=1calle1nombre.style --database=osm --username=$(whoami) spain-latest.osm.pbf

exit 0;
